              :data-source/data-source         #ig/ref :akvo.lumen.endpoint.data-source/data-source
              :env/env                         #ig/ref :akvo.lumen.endpoint.env/env
              :export/export                   #ig/ref :akvo.lumen.endpoint.export/export
              :files/files                     #ig/ref :akvo.lumen.endpoint.files/files
              :healthz/healthz                 #ig/ref :akvo.lumen.endpoint.healthz/healthz
              :invite/invite                   #ig/ref :akvo.lumen.endpoint.invite/invite
              :invite/verify                   #ig/ref :akvo.lumen.endpoint.invite/verify
              :job-execution/job-execution     #ig/ref :akvo.lumen.endpoint.job-execution/job-execution
              :library/library                 #ig/ref :akvo.lumen.endpoint.library/library
              :multiple-column/multiple-column #ig/ref :akvo.lumen.endpoint.multiple-column/multiple-column
              :public/public                   #ig/ref :akvo.lumen.endpoint.public/public
              :raster/raster                   #ig/ref :akvo.lumen.endpoint.raster/raster
              :resource/resource               #ig/ref :akvo.lumen.endpoint.resource/resource
              :root/root                       #ig/ref :akvo.lumen.endpoint.root/root
              :share/share                     #ig/ref :akvo.lumen.endpoint.share/share
              :split-column/endpoint           #ig/ref :akvo.lumen.endpoint.split-column/endpoint
              :tier/tier                       #ig/ref :akvo.lumen.endpoint.tier/tier
              :transformation/transformation   #ig/ref :akvo.lumen.endpoint.transformation/transformation
              :user/user                       #ig/ref :akvo.lumen.endpoint.user/user
              :visualisation/visualisation     #ig/ref :akvo.lumen.endpoint.visualisation/visualisation}
  :middleware {:functions {:hide-errors       #ig/ref :akvo.lumen.component.handler/wrap-hide-errors
                           :wrap-body         #ig/ref :akvo.lumen.component.handler/wrap-body
                           :wrap-response     #ig/ref :akvo.lumen.component.handler/wrap-response
                           :not-found         #ig/ref :akvo.lumen.component.handler/wrap-not-found
                           :ring-defaults     #ig/ref :akvo.lumen.component.handler/wrap-defaults
                           :wrap-auth         #ig/ref :akvo.lumen.auth/wrap-auth
                           :wrap-jwt          #ig/ref :akvo.lumen.auth/wrap-jwt
                           :wrap-label-tenant #ig/ref :akvo.lumen.component.tenant-manager/wrap-label-tenant
                           :wrap-sentry       #ig/ref :akvo.lumen.component.error-tracker/wrap-sentry
                           :monitoring  #ig/ref :akvo.lumen.monitoring/middleware

                           }
               :applied-fun #ig/ref :akvo.lumen.component.handler/applied-fun
               :applied   [:not-found :wrap-response :ring-defaults :wrap-body
                           :wrap-auth :wrap-jwt  :wrap-label-tenant :wrap-sentry
                           :hide-errors :monitoring]
               :arguments {:not-found     "Resource Not Found"
                           :hide-errors   "Internal Server Error"
                           :wrap-jwt      {:keycloak-url   keycloak-url
                                           :keycloak-realm keycloak-realm}
                           :ring-defaults {:params    {:urlencoded true
                                                       :keywordize true}
                                           :responses {:not-modified-responses true
                                                       :absolute-redirects     true
                                                       :content-types          true
                                                       :default-charset        "utf-8"}}
                           :wrap-sentry   [sentry-backend-dsn {:namespaces ["org.akvo" "akvo"]}]}}}

 :akvo.lumen.component.http/http      {:config #ig/ref :akvo.lumen.config
